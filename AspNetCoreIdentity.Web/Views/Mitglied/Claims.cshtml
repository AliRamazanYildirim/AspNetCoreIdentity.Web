
@{
    ViewData["Title"] = "Claims";
}
@model List<ClaimAnsichtModell>
<table class="table table-striped">
    <thead>
        <tr class="text-center">
            <th scope="col">Anbieter</th>
            <th scope="col">Typ</th>
            <th scope="col">Wert</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="text-center">
                <td>@item.Anbieter</td>
                <td>@item.Typ</td>
                <td>@item.Wert</td>
            </tr>
        }

    </tbody>
</table>
